CODE

const expertData = $input.first().json;

// Extrai apenas o que você precisa do full_data
const condensedExpert = {
  expert_id: expertData.expert_id,
  expert_name: expertData.expert_name,
  
  voice: {
    temperature: expertData.full_data.clone_config.temperature,
    formality: expertData.full_data.clone_config.formality_level,
    emotional_range: expertData.full_data.clone_config.emotional_range
  },
  
  archetype: {
    primary: {
      name: expertData.full_data.arquetipo_principal.name,
      weight: expertData.full_data.arquetipo_principal.percentage
    },
    secondary: {
      name: expertData.full_data.arquetipo_secundario.name,
      weight: expertData.full_data.arquetipo_secundario.percentage
    }
  },
  
  values: {
    core: expertData.full_data.valores_core.core_value.nome,
    supporting: expertData.full_data.valores_core.supporting_values.map(v => v.nome)
  },
  
  vocabulary: {
    always: expertData.full_data.vocabulario.sempre_usar,
    never: expertData.full_data.vocabulario.nunca_usar
  },
  
  signature_phrases: expertData.full_data.bordoes.principais.map(b => ({
    phrase: b.frase,
    context: b.quando_usar
  })),
  
  positioning: {
    promise: expertData.full_data.promessa_central.versao_principal,
    mechanism: expertData.full_data.big_idea.nome_mecanismo,
    tagline: expertData.full_data.big_idea.tagline,
    differentiator: expertData.full_data.diferenciacao.unico_diferenciador
  },
  
  // ✅ ADICIONE ESSAS LINHAS AQUI
  proposito: {
    declaracao: expertData.full_data.proposito.declaracao
  },
  
  missao: {
    declaracao: expertData.full_data.missao.declaracao
  },
  // ✅ FIM DA ADIÇÃO
  
  communication: {
    storytelling: expertData.full_data.comunicacao_avancada.storytelling,
    provocation_level: expertData.full_data.comunicacao_avancada.provocacao_humor.nivel,
    emoji_usage: expertData.full_data.comunicacao_avancada.uso_emojis.frequencia
  }
};

// Retorna como string JSON formatada
return {
  json: {
    formatted_json: JSON.stringify(condensedExpert, null, 2),
    raw_object: condensedExpert
  }
};

