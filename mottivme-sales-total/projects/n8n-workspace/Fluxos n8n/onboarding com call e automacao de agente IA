1ï¸âƒ£ DATABASE SCHEMA (SUPABASE/POSTGRESQL)
Tabela: clients
sql
CREATE TABLE clients (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  name TEXT NOT NULL,
  company TEXT,
  email TEXT UNIQUE,
  phone TEXT,
  plan TEXT CHECK (plan IN ('socialfy', 'bposs_basic', 'bposs_premium')),
  status TEXT CHECK (status IN ('onboarding', 'active', 'paused', 'churned')),
  onboarding_call_url TEXT,
  onboarding_call_date TIMESTAMP,
  created_at TIMESTAMP DEFAULT NOW(),
  updated_at TIMESTAMP DEFAULT NOW()
);

CREATE INDEX idx_clients_status ON clients(status);
Tabela: agent_versions â­ CORE DO VERSIONAMENTO
sql
CREATE TABLE agent_versions (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  client_id UUID REFERENCES clients(id) ON DELETE CASCADE,
  version TEXT NOT NULL, -- v1.0, v1.1, v2.0
  version_type TEXT CHECK (version_type IN ('onboarding', 'revision', 'hotfix')),
  
  -- CONFIGURAÃ‡Ã•ES DO AGENTE
  system_prompt TEXT NOT NULL,
  tools_config JSONB NOT NULL,
  personality_config JSONB NOT NULL,
  compliance_rules JSONB NOT NULL,
  qualification_flow JSONB NOT NULL,
  
  -- METADATA
  source_call_id UUID REFERENCES call_recordings(id),
  created_by TEXT, -- CS name
  approved_by TEXT,
  approved_at TIMESTAMP,
  is_active BOOLEAN DEFAULT FALSE, -- sÃ³ uma versÃ£o ativa por cliente
  
  -- MÃ‰TRICAS (preenchido depois)
  performance_metrics JSONB,
  
  created_at TIMESTAMP DEFAULT NOW(),
  
  UNIQUE(client_id, version)
);

CREATE INDEX idx_agent_versions_client ON agent_versions(client_id);
CREATE INDEX idx_agent_versions_active ON agent_versions(client_id, is_active) WHERE is_active = TRUE;
Tabela: call_recordings
sql
CREATE TABLE call_recordings (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  client_id UUID REFERENCES clients(id) ON DELETE CASCADE,
  call_type TEXT CHECK (call_type IN ('onboarding', 'revision')) NOT NULL,
  
  -- ARQUIVOS
  audio_url TEXT NOT NULL,
  transcript_url TEXT,
  transcript_text TEXT,
  
  -- PARTICIPANTES
  cs_name TEXT NOT NULL,
  duration_minutes INTEGER,
  
  -- ANÃLISE
  analysis_result JSONB, -- output do Claude
  extraction_status TEXT CHECK (extraction_status IN ('pending', 'processing', 'completed', 'failed')),
  
  -- VERSIONAMENTO
  generated_version_id UUID REFERENCES agent_versions(id),
  
  created_at TIMESTAMP DEFAULT NOW(),
  processed_at TIMESTAMP
);

CREATE INDEX idx_call_recordings_client ON call_recordings(client_id);
CREATE INDEX idx_call_recordings_status ON call_recordings(extraction_status);
Tabela: agent_knowledge_base
sql
CREATE TABLE agent_knowledge_base (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  client_id UUID REFERENCES clients(id) ON DELETE CASCADE,
  agent_version_id UUID REFERENCES agent_versions(id),
  
  -- DOCUMENTO
  document_type TEXT CHECK (document_type IN ('pdf', 'url', 'text', 'video')),
  document_url TEXT,
  document_title TEXT,
  document_content TEXT,
  
  -- EMBEDDINGS (se usar RAG)
  embedding VECTOR(1536), -- OpenAI ada-002 ou alternative
  
  -- METADATA
  is_active BOOLEAN DEFAULT TRUE,
  created_at TIMESTAMP DEFAULT NOW()
);

-- Para busca vetorial (se usar RAG)
CREATE INDEX idx_knowledge_base_embedding ON agent_knowledge_base 
USING ivfflat (embedding vector_cosine_ops);
Tabela: agent_conversations (tracking de performance)
sql
CREATE TABLE agent_conversations (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  client_id UUID REFERENCES clients(id),
  agent_version_id UUID REFERENCES agent_versions(id),
  
  -- LEAD INFO
  lead_phone TEXT,
  lead_name TEXT,
  
  -- CONVERSATION
  messages JSONB NOT NULL, -- array de mensagens
  start_time TIMESTAMP NOT NULL,
  end_time TIMESTAMP,
  
  -- OUTCOME
  qualified BOOLEAN,
  booked BOOLEAN,
  booking_url TEXT,
  disqualify_reason TEXT,
  
  -- ANÃLISE
  sentiment TEXT, -- positive/neutral/negative
  quality_score DECIMAL(3,2), -- 0.00 a 1.00
  
  created_at TIMESTAMP DEFAULT NOW()
);

CREATE INDEX idx_conversations_client ON agent_conversations(client_id);
CREATE INDEX idx_conversations_version ON agent_conversations(agent_version_id);
CREATE INDEX idx_conversations_outcome ON agent_conversations(qualified, booked);
Tabela: revision_requests (PDCA tracking)
sql
CREATE TABLE revision_requests (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  client_id UUID REFERENCES clients(id),
  call_id UUID REFERENCES call_recordings(id),
  current_version_id UUID REFERENCES agent_versions(id),
  
  -- PDCA STRUCTURE
  plan_expectations JSONB,
  do_actual_results JSONB,
  check_analysis JSONB,
  act_changes JSONB,
  
  -- PRIORIZAÃ‡ÃƒO
  priority TEXT CHECK (priority IN ('low', 'medium', 'high', 'urgent')),
  status TEXT CHECK (status IN ('pending', 'in_progress', 'implemented', 'rejected')),
  
  implemented_version_id UUID REFERENCES agent_versions(id),
  
  created_at TIMESTAMP DEFAULT NOW(),
  implemented_at TIMESTAMP
);
