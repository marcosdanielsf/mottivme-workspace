        }

        .sponsor-package h4 {
            font-family: var(--font-display);
            font-size: 24px;
            text-transform: uppercase;
            letter-spacing: 0.05em;
            color: var(--mf-red);
            margin-bottom: 15px;
        }

        .sponsor-package .price {
            font-family: var(--font-display);
            font-size: 36px;
            color: var(--mf-steel-100);
            margin-bottom: 20px;
