        .sponsor-packages {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            gap: 30px;
            margin-top: 40px;
        }

        .sponsor-package {
            background: var(--mf-black-deep);
            border: 1px solid rgba(196, 30, 58, 0.3);
            border-radius: 12px;
            padding: 30px;
            text-align: center;
            transition: all 300ms ease;
        }

        .sponsor-package:hover {
