        .sponsor-details h3 {
            font-family: var(--font-display);
            font-size: 28px;
            text-transform: uppercase;
            letter-spacing: 0.05em;
            color: var(--mf-red);
            margin-bottom: 15px;
        }

        .sponsor-details p {
            color: var(--mf-steel-80);
            margin-bottom: 20px;
            line-height: 1.6;
