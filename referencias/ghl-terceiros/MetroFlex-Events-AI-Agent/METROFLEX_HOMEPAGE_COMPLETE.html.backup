        .legacy {
            padding: 100px 20px;
            background: var(--mf-black-deep);
            position: relative;
            overflow: hidden;
        }

        /* Parallax Video Background - Marvel Effect */
        .legacy-video-bg {
            position: absolute;
            top: -10%;
            left: 0;
            width: 100%;
            height: 120%;
            z-index: 1;
            will-change: transform;
        }

        .legacy-video-bg video {
            width: 100%;
            height: 100%;
            object-fit: cover;
            opacity: 0.15;
            filter: grayscale(0.9) contrast(1.2);
        }

        .legacy-video-overlay {
            position: absolute;
            inset: 0;
            background: linear-gradient(
                180deg,
                var(--mf-black-deep) 0%,
                rgba(10, 14, 20, 0.7) 30%,
