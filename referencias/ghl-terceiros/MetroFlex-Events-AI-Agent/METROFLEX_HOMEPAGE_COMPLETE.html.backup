    <style>
        /* ============================================
           METROFLEX EVENTS - COMPLETE HOMEPAGE
           World-Class Premium Design
           ============================================ */

        /* DESIGN SYSTEM - CSS Variables */
        :root {
            /* Colors */
            --mf-black: #000000;
            --mf-black-deep: #0A0E14;
            --mf-black-surface: #141414;
            --mf-red: #C41E3A;
            --mf-red-light: #DC2626;
            --mf-red-dark: #991B2E;
            --mf-red-alpha: rgba(196, 30, 58, 0.4);
            --mf-steel-100: #FFFFFF;
            --mf-steel-95: #F5F5F5;
            --mf-steel-80: #C2C8CF;
            --mf-steel-70: #A8AFB6;
            --mf-gold: #FFD700;

            /* Typography */
            --font-display: 'Bebas Neue', 'Arial Black', sans-serif;
            --font-body: 'Inter', system-ui, sans-serif;

            /* Spacing */
            --space-2: 16px;
            --space-3: 24px;
            --space-4: 32px;
            --space-6: 48px;
            --space-8: 64px;
        }

        /* RESET & BASE STYLES */
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        html {
            scroll-behavior: smooth;
        }

        body {
            font-family: var(--font-body);
            background: var(--mf-black);
            color: var(--mf-steel-95);
            line-height: 1.6;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

        img {
            max-width: 100%;
            height: auto;
            display: block;
        }

        a {
            color: inherit;
            text-decoration: none;
        }

        /* UTILITY CLASSES */
        .container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 20px;
        }

        .sr-only {
            position: absolute;
            width: 1px;
            height: 1px;
            padding: 0;
            margin: -1px;
            overflow: hidden;
            clip: rect(0, 0, 0, 0);
            white-space: nowrap;
            border-width: 0;
        }

        /* NAVIGATION */
        nav {
            position: sticky;
            top: 0;
            z-index: 1000;
            background: rgba(20, 20, 20, 0.95);
            backdrop-filter: blur(10px);
            border-bottom: 1px solid rgba(196, 30, 58, 0.2);
        }

        .nav-container {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 20px 40px;
            max-width: 100%;
            margin: 0 auto;
        }

        .logo {
            font-family: var(--font-display);
            font-size: 28px;
            color: var(--mf-steel-100);
            text-transform: uppercase;
            letter-spacing: 0.1em;
        }

        .logo span {
            color: var(--mf-red);
        }

        .nav-links {
            display: flex;
            gap: 40px;
            list-style: none;
            align-items: center;
        }

        .nav-links a {
            color: var(--mf-steel-80);
            font-size: 14px;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.05em;
            transition: color 300ms;
        }

        .nav-links a:hover {
            color: var(--mf-red);
        }

        /* Dropdown Menu Styles */
        .dropdown {
            position: relative;
        }

        /* Add padding to dropdown parent to create hover bridge */
        .dropdown > a {
            padding-bottom: 20px;
        }

        .dropdown-menu {
            display: none;
            position: absolute;
            top: 100%;
            left: 0;
            background: rgba(20, 20, 20, 0.98);
            backdrop-filter: blur(10px);
            border: 1px solid rgba(196, 30, 58, 0.3);
            border-radius: 8px;
            padding: 12px 0;
            min-width: 220px;
            max-height: 400px;
            overflow-y: auto;
            margin-top: 0;
            box-shadow: 0 10px 40px rgba(0, 0, 0, 0.5);
            z-index: 9999;
        }

        /* Custom scrollbar for dropdown */
        .dropdown-menu::-webkit-scrollbar {
            width: 6px;
        }

        .dropdown-menu::-webkit-scrollbar-track {
            background: rgba(0, 0, 0, 0.3);
            border-radius: 3px;
        }

        .dropdown-menu::-webkit-scrollbar-thumb {
            background: var(--mf-red);
            border-radius: 3px;
        }

        .dropdown-menu::-webkit-scrollbar-thumb:hover {
            background: var(--mf-red-light);
        }

        .dropdown:hover .dropdown-menu,
        .dropdown-menu:hover {
            display: block;
        }

        .dropdown-menu li {
            list-style: none;
        }

        .dropdown-menu a {
            display: block;
            padding: 10px 20px;
            color: var(--mf-steel-80);
            transition: all 200ms;
            white-space: nowrap;
        }

        .dropdown-menu a:hover {
            background: rgba(196, 30, 58, 0.1);
            color: var(--mf-red);
            padding-left: 25px;
        }

        /* Mobile Menu Toggle */
        .mobile-menu-toggle {
            display: none;
            flex-direction: column;
            gap: 5px;
            background: transparent;
            border: none;
            padding: 8px;
            cursor: pointer;
            z-index: 1001;
        }

        .mobile-menu-toggle span {
            width: 28px;
            height: 3px;
            background: var(--mf-steel-95);
            border-radius: 2px;
            transition: all 300ms ease;
        }

        .mobile-menu-toggle.active span:nth-child(1) {
            transform: translateY(8px) rotate(45deg);
        }

        .mobile-menu-toggle.active span:nth-child(2) {
            opacity: 0;
        }

        .mobile-menu-toggle.active span:nth-child(3) {
            transform: translateY(-8px) rotate(-45deg);
        }

        /* Focus States - WCAG 2.1 Compliance */
        .btn:focus-visible {
            outline: 3px solid rgba(196, 30, 58, 0.8);
            outline-offset: 4px;
        }

        .nav-links a:focus-visible {
            outline: 2px solid var(--mf-red);
            outline-offset: 4px;
            border-radius: 4px;
        }

        .sponsor-tab:focus-visible,
        .floorplan-pin:focus-visible {
            outline: 3px solid rgba(196, 30, 58, 0.8);
            outline-offset: 3px;
        }

        .social-links a:focus-visible {
            outline: 3px solid var(--mf-red);
            outline-offset: 4px;
        }

        /* Scroll offset for sticky nav */
        section {
            scroll-margin-top: 80px;
        }

        /* HERO SECTION */
        .hero {
            position: relative;
            min-height: 100vh;
            display: flex;
            align-items: center;
            justify-content: center;
            overflow: hidden;
            background: var(--mf-black);
        }

        .hero-bg {
            position: absolute;
            inset: 0;
            background: linear-gradient(135deg, var(--mf-red) 0%, var(--mf-black-deep) 50%, var(--mf-black-surface) 100%);
            opacity: 0.3;
            filter: blur(120px);
        }

        .hero-content {
            position: relative;
            z-index: 10;
            text-align: center;
            max-width: 900px;
            padding: 100px 20px;
        }

        .eyebrow {
            display: block;
            font-size: 14px;
            font-weight: 700;
            letter-spacing: 0.2em;
            text-transform: uppercase;
            color: var(--mf-red);
            margin-bottom: var(--space-3);
        }

        .hero-title {
            font-family: var(--font-display);
            font-size: clamp(48px, 8vw, 96px);
            line-height: 1.1;
            letter-spacing: 0.05em;
            text-transform: uppercase;
            margin-bottom: var(--space-4);
            text-shadow: 0 0 40px rgba(196, 30, 58, 0.5), 0 4px 20px rgba(0, 0, 0, 0.8);
        }

        .hero-subtitle {
            font-family: var(--font-display);
            font-size: clamp(20px, 3vw, 28px);
            letter-spacing: 0.05em;
            text-transform: uppercase;
            color: var(--mf-steel-95);
            margin-bottom: var(--space-3);
        }

        .hero-description {
            font-size: clamp(16px, 2vw, 19px);
            color: var(--mf-steel-95);
            max-width: 700px;
            margin: 0 auto;
            margin-bottom: 50px;
            line-height: 1.7;
            font-weight: 400;
        }

        .highlight {
            color: var(--mf-red);
        }

        /* CTA BUTTONS */
        .cta-group {
            display: flex;
            gap: var(--space-3);
            justify-content: center;
            flex-wrap: wrap;
        }

        .btn {
            padding: 18px 40px;
            font-size: 14px;
            font-weight: 700;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            border-radius: 8px;
            transition: all 300ms;
            cursor: pointer;
        }

        .btn-primary {
            background: var(--mf-red);
            color: var(--mf-steel-100);
            border: none;
            box-shadow: 0 0 30px var(--mf-red-alpha);
        }

        .btn-primary:hover {
            background: var(--mf-red-light);
            transform: translateY(-4px);
            box-shadow: 0 0 50px var(--mf-red-alpha);
        }

        .btn-secondary {
            background: transparent;
            color: var(--mf-steel-95);
            border: 2px solid var(--mf-steel-95);
        }

        .btn-secondary:hover {
            background: rgba(196, 30, 58, 0.1);
            border-color: var(--mf-red);
            color: var(--mf-red);
        }

        /* EVENTS SECTION */
        .events {
            padding: 80px 20px;
            background: var(--mf-black-deep);
        }

        .section-header {
            text-align: center;
            margin-bottom: 60px;
        }

        .section-title {
            font-family: var(--font-display);
            font-size: clamp(36px, 6vw, 56px);
            text-transform: uppercase;
            letter-spacing: 0.05em;
            margin-bottom: 16px;
        }

        .section-subtitle {
            font-size: 18px;
            color: var(--mf-steel-70);
            max-width: 600px;
            margin: 0 auto;
        }

        .events-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 30px;
            max-width: 900px;
            margin: 0 auto;
        }

        .event-card {
            background: linear-gradient(135deg, var(--mf-black-surface), var(--mf-black));
            border: 2px solid rgba(196, 30, 58, 0.2);
            border-radius: 16px;
            padding: 40px;
            transition: all 300ms;
            position: relative;
            overflow: hidden;
        }

        .event-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 4px;
            background: linear-gradient(90deg, var(--mf-red), var(--mf-gold));
        }

        .event-card:hover {
            border-color: var(--mf-red);
            transform: translateY(-8px);
            box-shadow: 0 20px 60px rgba(196, 30, 58, 0.3);
        }

        .event-title {
            font-family: var(--font-display);
            font-size: 28px;
            text-transform: uppercase;
            letter-spacing: 0.05em;
            margin-bottom: 12px;
            color: var(--mf-steel-100);
        }

        .event-series {
            font-size: 14px;
            color: var(--mf-red);
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            margin-bottom: 20px;
        }

        .event-date {
            font-size: 16px;
            color: var(--mf-steel-80);
            margin-bottom: 12px;
        }

        .event-location {
            font-size: 14px;
            color: var(--mf-steel-70);
            margin-bottom: 20px;
        }

        .event-description {
            font-size: 15px;
            color: var(--mf-steel-80);
            line-height: 1.6;
            margin-bottom: 30px;
        }

        /* PARTNERS SECTION - Scrolling Text Banner */
        .partners {
            padding: 60px 0;
            background: var(--mf-black);
            overflow: hidden;
            position: relative;
        }

        .partners-marquee {
            width: 100%;
            overflow: hidden;
            position: relative;
        }

        .partners-track {
            display: flex;
            align-items: center;
            gap: 40px;
            animation: scroll 30s linear infinite;
            white-space: nowrap;
        }

        @keyframes scroll {
            0% {
                transform: translateX(0);
            }
            100% {
                transform: translateX(-50%);
            }
        }

        .partner-name {
            font-family: var(--font-display);
            font-size: 28px;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            color: var(--mf-steel-70);
            opacity: 0.5;
            transition: all 300ms ease;
        }

        .partner-name:hover {
            color: var(--mf-red);
            opacity: 1;
        }

        .partner-separator {
            font-size: 24px;
            color: var(--mf-red);
            opacity: 0.3;
        }

        @media (prefers-reduced-motion: reduce) {
            .partners-track {
                animation: none;
            }
