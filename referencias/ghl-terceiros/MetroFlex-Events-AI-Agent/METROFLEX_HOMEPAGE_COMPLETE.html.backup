    <!-- JavaScript for interactions and parallax effects -->
    <script>
        // Smooth scroll for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                const href = this.getAttribute('href');
                if (href.startsWith('#')) {
                    const target = document.querySelector(href);
                    if (target) {
                        e.preventDefault();
                        target.scrollIntoView({ behavior: 'smooth', block: 'start' });
                    }
                }
            });
        });

        // Parallax Video Background Effect - Marvel Cinematic Style
        function initParallax() {
            const legacySection = document.querySelector('.legacy');
            const videoBg = document.querySelector('.legacy-video-bg');

            if (!legacySection || !videoBg) return;

            let ticking = false;

            function updateParallax() {
                const rect = legacySection.getBoundingClientRect();
                const windowHeight = window.innerHeight;

                // Calculate scroll progress through section (-1 to 1)
                const scrollProgress = (windowHeight - rect.top) / (windowHeight + rect.height);

                // Apply parallax transform (slower movement creates depth)
                // Negative value moves video upward as you scroll down
                const translateY = -(scrollProgress * 100) * 0.3; // 0.3 = parallax speed

                videoBg.style.transform = `translateY(${translateY}px) scale(1.1)`;

                ticking = false;
            }

            function requestParallaxUpdate() {
                if (!ticking) {
                    window.requestAnimationFrame(updateParallax);
                    ticking = true;
                }
            }

            // Throttled scroll listener
            window.addEventListener('scroll', requestParallaxUpdate, { passive: true });

            // Initial parallax position
            updateParallax();
        }

        // Initialize on DOM ready
        if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', initParallax);
        } else {
            initParallax();
        }

        // Pause video on mobile for performance
        if (window.matchMedia('(max-width: 768px)').matches) {
            const videos = document.querySelectorAll('.legacy-video-bg video');
            videos.forEach(video => {
                video.pause();
                video.removeAttribute('autoplay');
            });
        }

        // Mobile Menu Toggle
        const mobileToggle = document.querySelector('.mobile-menu-toggle');
        const navLinks = document.querySelector('.nav-links');

        if (mobileToggle) {
            mobileToggle.addEventListener('click', () => {
                const isExpanded = mobileToggle.getAttribute('aria-expanded') === 'true';
                mobileToggle.setAttribute('aria-expanded', !isExpanded);
                mobileToggle.classList.toggle('active');
                navLinks.classList.toggle('active');
            });

            // Close menu when clicking outside
            document.addEventListener('click', (e) => {
                if (!e.target.closest('nav')) {
                    mobileToggle.classList.remove('active');
                    navLinks.classList.remove('active');
                    mobileToggle.setAttribute('aria-expanded', 'false');
                }
            });

            // Handle dropdown clicks on mobile
            document.querySelectorAll('.dropdown').forEach(dropdown => {
                const link = dropdown.querySelector('a');
                link.addEventListener('click', (e) => {
                    if (window.innerWidth <= 768) {
                        e.preventDefault();
                        dropdown.classList.toggle('active');
                    }
                });
            });
        }

        // Sponsorship Tabs Functionality
        const sponsorTabs = document.querySelectorAll('.sponsor-tab');
        const sponsorContents = document.querySelectorAll('.sponsor-content');

        sponsorTabs.forEach(tab => {
            tab.addEventListener('click', () => {
                // Remove active class from all tabs and contents
                sponsorTabs.forEach(t => t.classList.remove('active'));
                sponsorContents.forEach(c => c.classList.remove('active'));

                // Add active class to clicked tab
                tab.classList.add('active');

                // Show corresponding content
                const eventId = tab.dataset.event;
                const targetContent = document.getElementById(`sponsor-${eventId}`);
                if (targetContent) {
                    targetContent.classList.add('active');
                }
            });
        });

        // Floorplan Interactive Booth Selection
        document.querySelectorAll('[data-floorplan] .floorplan-pin').forEach((pin) => {
            pin.addEventListener('click', () => {
                const target = pin.dataset.zone;
                const details = document.querySelector('[data-floorplan-details]');

                switch (target) {
                    case 'booth-a1':
                        details.innerHTML = `
                            <h3>Booth A1 - Premium Corner</h3>
                            <p>Premium 10x10 corner booth. High traffic location near main entrance. Includes power outlet and 2 vendor badges.</p>
                            <p><strong>Price:</strong> $2,500</p>
                            <a href="#contact" class="btn btn-primary">Reserve Booth A1</a>
                        `;
                        break;

                    case 'booth-a2':
                        details.innerHTML = `
                            <h3>Booth A2 - Front Row</h3>
                            <p>10x10 booth in prime front row position. Great visibility and foot traffic. Includes power and 2 vendor badges.</p>
                            <p><strong>Price:</strong> $2,000</p>
                            <a href="#contact" class="btn btn-primary">Reserve Booth A2</a>
                        `;
                        break;

                    case 'booth-a3':
                        details.innerHTML = `
                            <h3>Booth A3 - Front Row</h3>
                            <p>10x10 booth in front row. Excellent exposure near registration area. Includes power and 2 vendor badges.</p>
                            <p><strong>Price:</strong> $2,000</p>
