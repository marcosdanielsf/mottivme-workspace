ulos de se√ß√£o, buscar a se√ß√£o\n  if (sectionTitles && sectionTitles.length > 0) {\n    for (const title of sectionTitles) {\n      const pattern = new RegExp(`${title}[:\\\\s]*([\\s\\S]*?)(?=###|##|\\\\n\\\\n\\\\*\\\\*|$)`, 'i');\n      const match = text.match(pattern);\n      if (match) {\n        section = match[1];\n        break;\n      }\n    }\n  }\n  \n  // Extrair bullets\n  const bullets = [...section.matchAll(/[-‚Ä¢‚úì‚úÖ‚ùå*]\\s*(.+?)(?=\\n|$)/g)];\n  return bullets.map(m => m[1].trim()).filter(b => b.length > 0);\n}\n\nfunction extractJSON(text) {\n  if (!text) return null;\n  \n  try {\n    // Tentar com ```json\n    const jsonMatch = text.match(/```json\\s*([\\s\\S]*?)\\s*```/);\n    if (jsonMatch) {\n      return JSON.parse(jsonMatch[1]);\n    }\n    \n    // Tentar JSON puro\n    if (text.trim().startsWith('{')) {\n      return JSON.parse(text);\n    }\n  } catch (e) {\n    return null;\n  }\n  \n  return null;\n}\n\nfunction extractQuotedPhrases(text) {\n  if (!text) return [];\n  \n  const quotes = [...text.matchAll(/\"([^\"]{10,})\"/g)];\n  return quotes.map(m => ({\n    frase: m[1],\n    quando_usar: \"Contexto a ser definido\"\n  }));\n}\n\nfunction findSectionText(text, sectionTitle) {\n  if (!text) return '';\n  \n  const pattern = new RegExp(`###\\\\s*${sectionTitle}[\\\\s\\\\S]*?(?=###|$)`, 'i');\n  const match = text.match(pattern);\n  return match ? match[0] : '';\n}\n\nfunction extractWordList(text, pattern) {\n  if (!text) return [];\n  \n  const regex = new RegExp(pattern + '[:\\\\s]*(.+?)(?=\\\\n\\\\*\\\\*|\\\\n\\\\n|$)', 'is');\n  const match = text.match(regex);\n  \n  if (!match) return [];\n  \n  const content = match[1];\n  \n  // Tentar extrair de lista entre colchetes [palavra1, palavra2]\n  const bracketMatch = content.match(/\\[(.+?)\\]/);\n  if (bracketMatch) {\n    return bracketMatch[1].split(',').map(w => w.trim().replace(/[\"\\s]/g, '')).filter(Boolean);\n  }\n  \n  // Tentar extrair palavras separadas por v√≠rgula\n  return content.split(',').map(w => w.trim()).filter(Boolean);\n}\n\nfunction extractAvataresFull(text) {\n  if (!text) return [];\n  \n  const avatares = [];\n  \n  // Buscar por \"## AVATAR X:\"\n  const avatarMatches = [...text.matchAll(/##\\s*AVATAR\\s*(\\d+):\\s*(.+?)\\n([\\s\\S]*?)(?=##\\s*AVATAR|$)/gi)];\n  \n  for (const match of avatarMatches) {\n    const num = match[1];\n    const nome = match[2].trim();\n    const content = match[3];\n    \n    const avatar = {\n      id: `avatar_${num}`,\n      nome: nome,\n      idade: extractDemografia(content, 'Idade'),\n      perfil: extractWithMarker(content, 'Ocupa√ß√£o'),\n      renda_mensal: extractDemografia(content, 'Renda'),\n      dor_externa: extractSubSection(content, 'Dor Externa'),\n      dor_real: extractSubSection(content, 'Dor Real'),\n      desejo_central: extractSubSection(content, 'Desejo Central'),\n      medo_oculto: extractSubSection(content, 'Medo Oculto'),\n      fit_score: null\n    };\n    \n    avatares.push(avatar);\n  }\n  \n  return avatares;\n}\n\nfunction extractDemografia(text, field) {\n  const pattern = new RegExp(`\\\\*\\\\*${field}:\\\\*\\\\*\\\\s*(.+?)(?=\\\\n|$)`, 'i');\n  const match = text.match(pattern);\n  return match ? match[1].trim() : null;\n}\n\nfunction extractSubSection(text, title) {\n  const pattern = new RegExp(`####\\\\s*${title}[\\\\s\\\\S]{0,20}(.{50,300})`, 'i');\n  const match = text.match(pattern);\n  return match ? match[1].trim() : null;\n}\n\nfunction extractMainPromise(text) {\n  if (!text) return null;\n  \n  // Formato: \"### Vers√£o Principal\\n> **texto**\"\n  const match = text.match(/###\\s*Vers√£o Principal[\\s\\S]*?>\\s*\\*\\*(.+?)\\*\\*/i);\n  return match ? match[1].trim() : null;\n}\n\nfunction extractChosenOption(text, section, field) {\n  if (!text) return null;\n  \n  // Formato: \"**ESCOLHIDA:** Op√ß√£o X\\n**Cliente:**\" ou \"**Transforma√ß√£o:**\"\n  const pattern = new RegExp(`DEFINI√á√ÉO DO \"${section}\"[\\\\s\\\\S]*?ESCOLHIDA[\\\\s\\\\S]*?\\\\*\\\\*${field}:\\\\*\\\\*\\\\s*(.+?)(?=\\\\n\\\\*\\\\*|\\\\n\\\\n|$)`, 'i');\n  const match = text.match(pattern);\n  return match ? match[1].trim() : null;\n}\n\nfunction extractVersion(text, versionLabel, typeLabel) {\n  if (!text) return null;\n  \n  const pattern = new RegExp(`###\\\\s*${versionLabel}:\\\\s*${typeLabel}[\\\\s\\\\S]*?>\\\\s*(.+?)(?=\\\\n\\\\n|\\\\*\\\\*|###)`, 'i');\n  const match = text.match(pattern);\n  return match ? match[1].trim() : null;\n}\n\nfunction extractWithMarker(text, marker) {\n  if (!text) return null;\n  \n  const pattern = new RegExp(`\\\\*\\\\*${marker}:\\\\*\\\\*\\\\s*(.+?)(?=\\\\n\\\\n|\\\\*\\\\*|###|$)`, 'is');\n  const match = text.match(pattern);\n  return match ? match[1].trim() : null;\n}\n\nfunction extractExplanation(text, type) {\n  if (!text) return null;\n  \n  const pattern = new RegExp(`####\\\\s*${type}[\\\\s\\\\S]{0,50}(.{100,500})`, 'i');\n  const match = text.match(pattern);\n  return match ? match[1].trim() : null;\n}\n\nfunction extractPilares(text) {\n  if (!text) return [];\n  \n  // Buscar por \"**Pilar X:\"\n  const pilares = [...text.matchAll(/\\*\\*Pilar\\s*\\d+:\\s*(.+?)\\*\\*/gi)];\n  return pilares.map(m => m[1].trim());\n}\n\nfunction extractOfertas(text, keyword, type) {\n  if (!text) return [];\n  \n  const ofertas = [];\n  const pattern = new RegExp(`##\\\\s*${keyword}\\\\s*(\\\\d+)[\\\\s\\\\S]*?(?=##\\\\s*${keyword}|$)`, 'gi');\n  const matches = [...text.matchAll(pattern)];\n  \n  for (const match of matches) {\n    const num = match[1];\n    const content = match[0];\n    \n    const nome = extractWithMarker(content, 'Nome') || content.match(/###\\s*(.+?)(?=\\n|$)/)?.[1]?.trim();\n    const precoMatch = content.match(/pre√ßo.*?R\\$\\s*([\\d.,]+)/i);\n    \n    ofertas.push({\n      id: `${type}_${num}`,\n      nome: nome,\n      preco: precoMatch ? parseFloat(precoMatch[1].replace(/\\./g, '').replace(',', '.')) : null,\n      publico: extractWithMarker(content, 'P√∫blico')\n    });\n  }\n  \n  return ofertas;\n}\n\nfunction extractNumber(text, pattern) {\n  if (!text) return null;\n  const match = text.match(pattern);\n  if (!match) return null;\n  return parseFloat(match[1].replace(/\\./g, '').replace(',', '.'));\n}\n\nfunction extractWithUnit(text, pattern) {\n  if (!text) return null;\n  const match = text.match(pattern);\n  return match ? match[1].trim() : null;\n}\n\nreturn enrichedData;"
