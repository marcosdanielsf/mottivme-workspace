        "jsCode": "  // PROCESSAR TODOS OS STORIES\n\n  const items = $input.all();\n\n  console.log('==============================================');\n  console.log('PROCESSANDO STORIES');\n  console.log('==============================================');\n  console.log('Total de items recebidos:', items.length);\n\n  // PEGAR EXPERT_ID\n  let expertId = null;\n\n  try {\n    expertId = $('Get Expert DNA2').first().json.id;\n    console.log('Expert ID encontrado:', expertId);\n  } catch (error) {\n    console.log('Tentando buscar Expert ID do item...');\n    try {\n      expertId = items[0].json.expert_id || items[0].json.id;\n    } catch (error2) {\n      console.log('Nao foi possivel encontrar Expert ID');\n    }\n  }\n\n  // PROCESSA CADA SEMANA\n  const semanas = {};\n\n  items.forEach((item, index) => {\n    const agentOutput = item.json?.output;\n    const semana = item.json?.semana;\n\n    console.log('\\nProcessando Item', index + 1, '- Semana', semana);\n\n    if (!agentOutput || !semana) {\n      console.log('  AVISO: output ou semana nao encontrado');\n      return;\n    }\n\n    // Remove markdown code blocks\n    let cleaned = typeof agentOutput === 'string'\n      ? agentOutput.replace(/```json\\s*/g, '').replace(/```\\s*/g, '').trim()\n      : JSON.stringify(agentOutput);\n\n    try {\n      const parsed = JSON.parse(cleaned);\n\n      console.log('  Estrutura detectada:', Object.keys(parsed));\n      console.log('  E array?', Array.isArray(parsed));\n\n      let stories = [];\n\n      // DETECTA ESTRUTURA\n\n      // Caso 1: Objeto com propriedade \"stories\" (plural)\n      if (parsed.stories && Array.isArray(parsed.stories)) {\n        console.log('  Estrutura: Objeto com stories array');\n        stories = parsed.stories;\n      }\n      // Caso 2: Objeto com \"stories_semana_X\" (Semana 3 com estrutura diferente)\n      else if (parsed.stories_semana_1 || parsed.stories_semana_2 || parsed.stories_semana_3 ||\n  parsed.stories_semana_4) {\n        console.log('  Estrutura: Objeto com stories_semana_X');\n        const storiesKey = Object.keys(parsed).find(key => key.startsWith('stories_semana_'));\n        if (storiesKey && Array.isArray(parsed[storiesKey])) {\n          stories = parsed[storiesKey];\n        }\n      }\n      // Caso 3: Objeto com propriedade \"story\" (singular) que eh array\n      else if (parsed.story && Array.isArray(parsed.story)) {\n        console.log('  Estrutura: Objeto com story array');\n        stories = parsed.story;\n      }\n      // Caso 4: Objeto com propriedade \"story\" (singular) que eh objeto\n      else if (parsed.story && typeof parsed.story === 'object') {\n        console.log('  Estrutura: Objeto com story objeto');\n        stories = [parsed.story];\n      }\n      // Caso 5: Array direto\n      else if (Array.isArray(parsed)) {\n        console.log('  Estrutura: Array direto');\n        stories = parsed;\n      }\n\n      console.log('  Stories encontrados:', stories.length);\n\n      if (stories.length === 0) {\n        console.log('  Sem stories validos');\n        semanas['semana_' + semana] = {\n          stories_texto: '',\n          stories_json: '[]',\n          total_stories: 0\n        };\n        return;\n      }\n\n      // FORMATA OS STORIES\n      // Extrai story_sequence se existir\n      const storiesProcessados = stories.map(s => s.story_sequence || s);\n\n      const storiesFormatados = storiesProcessados.map((s) => {\n        const dia = s.dia_trilha || 'N/A';\n        const totalCards = s.cards ? s.cards.length : 0;\n\n        let resultado = '================================================================\\n';\n        resultado += 'STORY DIA ' + dia + ' | ' + (s.tema || 'N/A') + '\\n';\n        resultado += '================================================================\\n\\n';\n        resultado += 'FORMATO: ' + (s.formato || 'N/A') + '\\n';\n        resultado += 'OBJETIVO: ' + (s.objetivo || 'N/A') + '\\n';\n        resultado += 'TEMPLATE: ' + (s.template_usado || 'N/A') + '\\n';\n        resultado += 'DURACAO TOTAL: ' + (s.duracao_total_estimada || 'N/A') + '\\n\\n';\n        resultado += 'TOTAL DE CARDS: ' + totalCards + '\\n\\n';\n\n        // CARDS\n        if (s.cards && s.cards.length > 0) {\n          resultado += 'CARDS:\\n\\n';\n\n          s.cards.forEach(card => {\n            resultado += '  CARD ' + card.numero + ':\\n';\n            resultado += '  Tipo: ' + (card.tipo || 'N/A') + '\\n';\n            resultado += '  Duracao: ' + (card.duracao_segundos || 0) + 's\\n';\n\n            // Visual\n            if (card.visual) {\n              resultado += '  Visual:\\n';\n              resultado += '    - Tipo: ' + (card.visual.tipo || 'N/A') + '\\n';\n              resultado += '    - Fundo: ' + (card.visual.fundo || 'N/A') + '\\n';\n              if (card.visual.elementos && card.visual.elementos.length > 0) {\n                resultado += '    - Elementos: ' + card.visual.elementos.join(', ') + '\\n';\n              }\n            }\n\n            // Conteudo\n            if (card.conteudo) {\n              resultado += '  Conteudo:\\n';\n              if (card.conteudo.titulo) resultado += '    - Titulo: ' + card.conteudo.titulo + '\\n';\n              if (card.conteudo.texto_principal) resultado += '    - Texto: ' + card.conteudo.texto_principal +\n  '\\n';\n              if (card.conteudo.subtexto) resultado += '    - Subtexto: ' + card.conteudo.subtexto + '\\n';\n              if (card.conteudo.emoji) resultado += '    - Emoji: ' + card.conteudo.emoji + '\\n';\n\n              if (card.conteudo.bullets && card.conteudo.bullets.length > 0) {\n                resultado += '    - Bullets:\\n';\n                card.conteudo.bullets.forEach(b => {\n                  resultado += '       ' + b + '\\n';\n                });\n              }\n            }\n\n            if (card.objetivo_card) {\n              resultado += '  Objetivo: ' + card.objetivo_card + '\\n';\n            }\n\n            resultado += '\\n';\n          });\n        }\n\n        // CTA FINAL\n        if (s.cta_final) {\n          resultado += 'CTA FINAL:\\n';\n          resultado += '  Tipo: ' + (s.cta_final.tipo || 'N/A') + '\\n';\n          resultado += '  Texto: ' + (s.cta_final.texto_cta || 'N/A') + '\\n';\n          if (s.cta_final.palavra_chave) {\n            resultado += '  Palavra-chave: ' + s.cta_final.palavra_chave + '\\n';\n          }\n          resultado += '  Acao esperada: ' + (s.cta_final.acao_esperada || 'N/A') + '\\n\\n';\n        }\n\n        // METRICAS\n        if (s.metricas_esperadas) {\n          resultado += 'METRICAS ESPERADAS:\\n';\n          resultado += '  Visualizacoes completas: ' + (s.metricas_esperadas.visualizacoes_completas || 'N/A') +\n  '\\n';\n          resultado += '  Interacoes: ' + (s.metricas_esperadas.interacoes || 'N/A') + '\\n';\n          resultado += '  Conversoes: ' + (s.metricas_esperadas.conversoes || 'N/A') + '\\n';\n        }\n\n        return resultado;\n      }).join('\\n\\n');\n\n      semanas['semana_' + semana] = {\n        stories_texto: storiesFormatados,\n        stories_json: JSON.stringify(storiesProcessados, null, 2),\n        total_stories: storiesProcessados.length\n      };\n\n      console.log('  Stories formatados:', storiesProcessados.length);\n\n    } catch (error) {\n      console.error('  ERRO ao processar semana', semana, ':', error.message);\n      semanas['semana_' + semana] = {\n        stories_texto: '',\n        stories_json: '[]',\n        total_stories: 0\n      };\n    }\n  });\n\n  // COMBINA TODOS OS STORIES\n  const todosStories = [1, 2, 3, 4]\n    .map(num => {\n      const s = semanas['semana_' + num];\n      if (!s || !s.stories_texto) return '';\n\n      return '\\n\\n================================================================\\n' +\n             'SEMANA ' + num + '\\n' +\n             '================================================================\\n\\n' +\n             s.stories_texto;\n    })\n    .filter(t => t.length > 0)\n    .join('\\n\\n');\n\n  const todosStoriesJSON = [1, 2, 3, 4]\n    .flatMap(num => {\n      const s = semanas['semana_' + num];\n      if (!s || !s.stories_json) return [];\n      try {\n        return JSON.parse(s.stories_json);\n      } catch {\n        return [];\n      }\n    });\n\n  const totalStories = Object.values(semanas).reduce((sum, s) => sum + (s.total_stories || 0), 0);\n\n  console.log('');\n  console.log('==============================================');\n  console.log('PROCESSAMENTO COMPLETO');\n  console.log('==============================================');\n  console.log('Total de stories:', totalStories);\n  console.log('');\n  console.log('Stories por semana:');\n  console.log('  Semana 1:', (semanas.semana_1?.total_stories || 0), 'stories');\n  console.log('  Semana 2:', (semanas.semana_2?.total_stories || 0), 'stories');\n  console.log('  Semana 3:', (semanas.semana_3?.total_stories || 0), 'stories');\n  console.log('  Semana 4:', (semanas.semana_4?.total_stories || 0), 'stories');\n  console.log('==============================================');\n\n  return {\n    json: {\n      expert_id: expertId,\n      stories_completo_texto: todosStories,\n      stories_completo_json: JSON.stringify(todosStoriesJSON, null, 2),\n      stories_semana_1: semanas.semana_1?.stories_texto || '',\n      stories_semana_2: semanas.semana_2?.stories_texto || '',\n      stories_semana_3: semanas.semana_3?.stories_texto || '',\n      stories_semana_4: semanas.semana_4?.stories_texto || '',\n      total_stories: totalStories,\n      data_processamento: new Date().toISOString()\n    }\n  };"
