_esperadas.shares || 'N/A') + '\\n';\n          resultado += '  Conversoes: ' + (p.metricas_esperadas.conversoes || 'N/A') + '\\n';\n        }\n\n        if (p.notas_estrategicas) {\n          resultado += '\\nNOTAS ESTRATEGICAS:\\n';\n          resultado += '  Melhor horario: ' + (p.notas_estrategicas.melhor_dia_horario || 'N/A') + '\\n';\n          resultado += '  Tipo audiencia: ' + (p.notas_estrategicas.tipo_audiencia || 'N/A') + '\\n';\n          resultado += '  Sensibilidade: ' + (p.notas_estrategicas.sensibilidade || 'N/A') + '\\n';\n        }\n\n        return resultado;\n      }).join('\\n\\n');\n\n      semanas['semana_' + semana] = {\n        posts_texto: postsFormatados,\n        posts_json: JSON.stringify(posts, null, 2),\n        total_posts: posts.length\n      };\n\n      console.log('  Posts formatados:', posts.length);\n\n    } catch (error) {\n      console.error('  ERRO ao processar semana', semana, ':', error.message);\n      semanas['semana_' + semana] = {\n        posts_texto: '',\n        posts_json: '[]',\n        total_posts: 0\n      };\n    }\n  });\n\n  // COMBINA TODOS OS POSTS\n  const todosPosts = [1, 2, 3, 4]\n    .map(num => {\n      const s = semanas['semana_' + num];\n      if (!s || !s.posts_texto) return '';\n\n      return '\\n\\n================================================================\\n' +\n             'SEMANA ' + num + '\\n' +\n             '================================================================\\n\\n' +\n             s.posts_texto;\n    })\n    .filter(t => t.length > 0)\n    .join('\\n\\n');\n\n  const todosPostsJSON = [1, 2, 3, 4]\n    .flatMap(num => {\n      const s = semanas['semana_' + num];\n      if (!s || !s.posts_json) return [];\n      try {\n        return JSON.parse(s.posts_json);\n      } catch {\n        return [];\n      }\n    });\n\n  const totalPosts = Object.values(semanas).reduce((sum, s) => sum + (s.total_posts || 0), 0);\n\n  console.log('');\n  console.log('==============================================');\n  console.log('PROCESSAMENTO COMPLETO');\n  console.log('==============================================');\n  console.log('Total de posts:', totalPosts);\n  console.log('');\n  console.log('Posts por semana:');\n  console.log('  Semana 1:', (semanas.semana_1?.total_posts || 0), 'posts');\n  console.log('  Semana 2:', (semanas.semana_2?.total_posts || 0), 'posts');\n  console.log('  Semana 3:', (semanas.semana_3?.total_posts || 0), 'posts');\n  console.log('  Semana 4:', (semanas.semana_4?.total_posts || 0), 'posts');\n  console.log('==============================================');\n\n  return {\n    json: {\n      expert_id: expertId,\n      posts_completo_texto: todosPosts,\n      posts_completo_json: JSON.stringify(todosPostsJSON, null, 2),\n      posts_semana_1: semanas.semana_1?.posts_texto || '',\n      posts_semana_2: semanas.semana_2?.posts_texto || '',\n      posts_semana_3: semanas.semana_3?.posts_texto || '',\n      posts_semana_4: semanas.semana_4?.posts_texto || '',\n      total_posts: totalPosts,\n      data_processamento: new Date().toISOString()\n    }\n  };"
