        "jsCode": "// PROCESSAR TODOS OS CARROSSEIS\n\n  const items = $input.all();\n\n  console.log('==============================================');\n  console.log('PROCESSANDO CARROSSEIS');\n  console.log('==============================================');\n  console.log('Total de items recebidos:', items.length);\n\n  // PEGAR EXPERT_ID\n  let expertId = null;\n\n  try {\n    expertId = $('Get Expert DNA2').first().json.id;\n    console.log('Expert ID encontrado:', expertId);\n  } catch (error) {\n    console.log('Tentando buscar Expert ID do item...');\n    try {\n      expertId = items[0].json.expert_id || items[0].json.id;\n    } catch (error2) {\n      console.log('Nao foi possivel encontrar Expert ID');\n    }\n  }\n\n  // PROCESSA CADA SEMANA\n  const semanas = {};\n\n  items.forEach((item, index) => {\n    const agentOutput = item.json?.output;\n    const semana = item.json?.semana;\n\n    console.log('\\nProcessando Item', index + 1, '- Semana', semana);\n\n    if (!agentOutput || !semana) {\n      console.log('  AVISO: output ou semana nao encontrado');\n      return;\n    }\n\n    // Remove markdown code blocks\n    let cleaned = typeof agentOutput === 'string'\n      ? agentOutput.replace(/```json\\s*/g, '').replace(/```\\s*/g, '').trim()\n      : JSON.stringify(agentOutput);\n\n    try {\n      const parsed = JSON.parse(cleaned);\n\n      console.log('  Estrutura detectada:', Object.keys(parsed));\n      console.log('  E array?', Array.isArray(parsed));\n\n      let carrosseis = [];\n\n      // DETECTA ESTRUTURA\n\n      // Caso 1: Array direto de carrosseis (Semanas 1 e 3)\n      if (Array.isArray(parsed)) {\n        console.log('  Estrutura: Array direto');\n        carrosseis = parsed.map(item => item.carrossel).filter(c => c);\n      }\n      // Caso 2: Objeto com propriedade \"carrosseis\" (plural) - Semana 4\n      else if (parsed.carrosseis && Array.isArray(parsed.carrosseis)) {\n        console.log('  Estrutura: Objeto com carrosseis (plural) array');\n        carrosseis = parsed.carrosseis.map(item => item.carrossel).filter(c => c);\n      }\n      // Caso 3: Objeto com propriedade \"carrossel\" que eh array (Semana 3 alternativa)\n      else if (parsed.carrossel && Array.isArray(parsed.carrossel)) {\n        console.log('  Estrutura: Objeto com carrossel array');\n        carrosseis = parsed.carrossel;\n      }\n      // Caso 4: Objeto com propriedade \"carrossel\" que eh objeto (Semana 2)\n      else if (parsed.carrossel && typeof parsed.carrossel === 'object') {\n        console.log('  Estrutura: Objeto com carrossel objeto');\n        carrosseis = [parsed.carrossel];\n      }\n\n      console.log('  Carrosseis encontrados:', carrosseis.length);\n\n      if (carrosseis.length === 0) {\n        console.log('  Sem carrosseis validos');\n        semanas['semana_' + semana] = {\n          carrosseis_texto: '',\n          carrosseis_json: '[]',\n          total_carrosseis: 0\n        };\n        return;\n      }\n\n      // FORMATA OS CARROSSEIS\n      const carrosseisFormatados = carrosseis.map((c) => {\n        const dia = c.dia_trilha || 'N/A';\n        const totalSlides = c.slides ? c.slides.length : 0;\n\n        let resultado = '================================================================\\n';\n        resultado += 'CARROSSEL DIA ' + dia + ' | ' + (c.tema || 'N/A') + '\\n';\n        resultado += '================================================================\\n\\n';\n        resultado += 'OBJETIVO: ' + (c.objetivo || 'N/A') + '\\n';\n        resultado += 'TEMPLATE: ' + (c.template_usado || 'N/A') + '\\n\\n';\n        resultado += 'TOTAL DE SLIDES: ' + totalSlides + '\\n\\n';\n\n        // SLIDES\n        if (c.slides && c.slides.length > 0) {\n          resultado += 'SLIDES:\\n\\n';\n\n          c.slides.forEach(slide => {\n            resultado += '  SLIDE ' + slide.numero + ':\\n';\n            resultado += '  Tipo: ' + (slide.tipo || 'N/A') + '\\n';\n\n            if (slide.titulo) resultado += '  Titulo: ' + slide.titulo + '\\n';\n            if (slide.titulo_slide) resultado += '  Titulo: ' + slide.titulo_slide + '\\n';\n            if (slide.subtitulo) resultado += '  Subtitulo: ' + slide.subtitulo + '\\n';\n            if (slide.conteudo) resultado += '  Conteudo: ' + slide.conteudo + '\\n';\n\n            if (slide.bullets && slide.bullets.length > 0) {\n              resultado += '  Bullets:\\n';\n              slide.bullets.forEach(b => {\n                resultado += '     - ' + b + '\\n';\n              });\n            }\n\n            if (slide.elementos_visuais_sugeridos) {\n              resultado += '  Visual: ' + slide.elementos_visuais_sugeridos + '\\n';\n            }\n            if (slide.nota_design) {\n              resultado += '  Design: ' + slide.nota_design + '\\n';\n            }\n\n            resultado += '\\n';\n          });\n        }\n\n        // LEGENDA\n        if (c.legenda) {\n          resultado += 'LEGENDA:\\n';\n          resultado += '  Gancho: ' + (c.legenda.gancho || 'N/A') + '\\n';\n          resultado += '  Corpo: ' + (c.legenda.corpo || 'N/A') + '\\n';\n          resultado += '  CTA: ' + (c.legenda.cta || 'N/A') + '\\n';\n\n          if (c.legenda.hashtags && c.legenda.hashtags.length > 0) {\n            resultado += '  Hashtags: ' + c.legenda.hashtags.join(' ') + '\\n';\n          }\n          resultado += '\\n';\n        }\n\n        // METRICAS\n        if (c.metricas_esperadas) {\n          resultado += 'METRICAS ESPERADAS:\\n';\n          resultado += '  Saves: ' + (c.metricas_esperadas.saves || 'N/A') + '\\n';\n          resultado += '  Shares: ' + (c.metricas_esperadas.shares || 'N/A') + '\\n';\n          resultado += '  Comentarios: ' + (c.metricas_esperadas.comentarios || 'N/A') + '\\n';\n          resultado += '  Conversoes: ' + (c.metricas_esperadas.conversoes || 'N/A') + '\\n';\n        }\n\n        return resultado;\n      }).join('\\n\\n');\n\n      semanas['semana_' + semana] = {\n        carrosseis_texto: carrosseisFormatados,\n        carrosseis_json: JSON.stringify(carrosseis, null, 2),\n        total_carrosseis: carrosseis.length\n      };\n\n      console.log('  Carrosseis formatados:', carrosseis.length);\n\n    } catch (error) {\n      console.error('  ERRO ao processar semana', semana, ':', error.message);\n      semanas['semana_' + semana] = {\n        carrosseis_texto: '',\n        carrosseis_json: '[]',\n        total_carrosseis: 0\n      };\n    }\n  });\n\n  // COMBINA TODOS OS CARROSSEIS\n  const todosCarrosseis = [1, 2, 3, 4]\n    .map(num => {\n      const s = semanas['semana_' + num];\n      if (!s || !s.carrosseis_texto) return '';\n\n      return '\\n\\n================================================================\\n' +\n             'SEMANA ' + num + '\\n' +\n             '================================================================\\n\\n' +\n             s.carrosseis_texto;\n    })\n    .filter(t => t.length > 0)\n    .join('\\n\\n');\n\n  const todosCarrosseisJSON = [1, 2, 3, 4]\n    .flatMap(num => {\n      const s = semanas['semana_' + num];\n      if (!s || !s.carrosseis_json) return [];\n      try {\n        return JSON.parse(s.carrosseis_json);\n      } catch {\n        return [];\n      }\n    });\n\n  const totalCarrosseis = Object.values(semanas).reduce((sum, s) => sum + (s.total_carrosseis || 0), 0);\n\n  console.log('');\n  console.log('==============================================');\n  console.log('PROCESSAMENTO COMPLETO');\n  console.log('==============================================');\n  console.log('Total de carrosseis:', totalCarrosseis);\n  console.log('');\n  console.log('Carrosseis por semana:');\n  console.log('  Semana 1:', (semanas.semana_1?.total_carrosseis || 0), 'carrosseis');\n  console.log('  Semana 2:', (semanas.semana_2?.total_carrosseis || 0), 'carrosseis');\n  console.log('  Semana 3:', (semanas.semana_3?.total_carrosseis || 0), 'carrosseis');\n  console.log('  Semana 4:', (semanas.semana_4?.total_carrosseis || 0), 'carrosseis');\n  console.log('==============================================');\n\n  return {\n    json: {\n      expert_id: expertId,\n      carrosseis_completo_texto: todosCarrosseis,\n      carrosseis_completo_json: JSON.stringify(todosCarrosseisJSON, null, 2),\n      carrosseis_semana_1: semanas.semana_1?.carrosseis_texto || '',\n      carrosseis_semana_2: semanas.semana_2?.carrosseis_texto || '',\n      carrosseis_semana_3: semanas.semana_3?.carrosseis_texto || '',\n      carrosseis_semana_4: semanas.semana_4?.carrosseis_texto || '',\n      total_carrosseis: totalCarrosseis,\n      data_processamento: new Date().toISOString()\n    }\n  };"
