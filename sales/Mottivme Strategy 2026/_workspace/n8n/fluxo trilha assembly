          });\n        }\n      }\n      \n      // Anti-valores\n      const antiMatch = section.match(/### Anti-Valores[\\s\\S]*?(?=###|$)/i);\n      if (antiMatch) {\n        valores.anti_values = extractBullets(antiMatch[0], null);\n      }\n    }\n  }\n  \n  // ===== 4. BORD√ïES =====\n  const bordoes = {\n    principais: extractQuotedPhrases(identityMap),\n    estruturas_linguisticas: {\n      abertura: extractQuotedPhrases(findSectionText(identityMap, 'Frases de Abertura')),\n      transicao: extractQuotedPhrases(findSectionText(identityMap, 'Frases de Transi√ß√£o')),\n      fechamento: extractQuotedPhrases(findSectionText(identityMap, 'Frases de Fechamento'))\n    }\n  };\n  \n  // ===== 5. VOCABUL√ÅRIO =====\n  const vocabulario = {\n    sempre_usar: extractWordList(identityMap, 'sempre.*?usar'),\n    nunca_usar: extractWordList(identityMap, 'nunca.*?usar')\n  };\n  \n  // ===== 6. AVATARES =====\n  const avataresData = extractAvataresFull(avatares);\n  \n  // ===== 7. PROMESSA CENTRAL =====\n  const promessa = {\n    versao_principal: extractMainPromise(promessaCentral),\n    x_quem: extractChosenOption(promessaCentral, 'X', 'Cliente'),\n    y_transformacao: extractChosenOption(promessaCentral, 'Y', 'Transforma√ß√£o'),\n    versoes_alternativas: {\n      formal: extractVersion(promessaCentral, 'Vers√£o 1', 'Formal'),\n      conversacional: extractVersion(promessaCentral, 'Vers√£o 2', 'Conversacional'),\n      emocional: extractVersion(promessaCentral, 'Vers√£o 3', 'Emocional'),\n      aspiracional: extractVersion(promessaCentral, 'Vers√£o 4', 'Aspiracional'),\n      provocativa: extractVersion(promessaCentral, 'Vers√£o 5', 'Provocativa')\n    }\n  };\n  \n  // ===== 8. BIG IDEA =====\n  const bigIdeaData = {\n    nome_mecanismo: extractWithMarker(bigIdea, 'Nome do Mecanismo'),\n    tagline: extractWithMarker(bigIdea, 'Tagline'),\n    explicacao_simples: extractExplanation(bigIdea, 'Explica√ß√£o Simples'),\n    explicacao_tecnica: extractExplanation(bigIdea, 'Explica√ß√£o T√©cnica'),\n    pilares: extractPilares(bigIdea)\n  };\n  \n  // ===== 9. PMV =====\n  const proposito = {\n    declaracao: extractWithMarker(identityMap, 'Declara√ß√£o de Prop√≥sito'),\n    expandido: extractWithMarker(identityMap, 'Prop√≥sito Expandido')\n  };\n  \n  const missao = {\n    declaracao: extractWithMarker(identityMap, 'Declara√ß√£o de Miss√£o'),\n    como_realiza: extractBullets(findSectionText(identityMap, 'Como realiza'), null)\n  };\n  \n  const visao = {\n    futuro: extractWithMarker(identityMap, 'Vis√£o de Futuro'),\n    impacto_longo_prazo: extractWithMarker(identityMap, 'Impacto de Longo Prazo')\n  };\n  \n  // ===== 10. DIFERENCIA√á√ÉO =====\n  const diferenciacao = {\n    unico_diferenciador: extractWithMarker(identityMap, '√önico Diferenciador'),\n    elementos: extractBullets(findSectionText(identityMap, 'Elementos de Diferencia√ß√£o'), null).map(e => ({descricao: e})),\n    vs_concorrencia: extractWithMarker(identityMap, 'Como √© diferente')\n  };\n  \n  // ===== 11. OFERTAS =====\n  const ofertas = {\n    high_ticket: extractOfertas(ofertasHigh, 'OFERTA', 'high'),\n    back_end: extractOfertas(ofertasBack, 'OFERTA', 'back'),\n    front_end: extractOfertas(ofertasFront, 'ISCA', 'front')\n  };\n  \n  // ===== 12. COMUNICA√á√ÉO =====\n  const comunicacao = {\n    primeira_vs_segunda_pessoa: extractWithMarker(identityMap, 'Primeira vs Segunda Pessoa'),\n    storytelling: extractWithMarker(identityMap, 'Storytelling'),\n    provocacao_humor: {\n      nivel: extractWithMarker(identityMap, 'Provoca√ß√£o e Humor'),\n      estilo: null\n    },\n    uso_emojis: {\n      frequencia: extractEmojisFrequency(cloneConfig),\n      razao: null\n    }\n  };\n  \n  // ===== 13. ESTRAT√âGIA TR√ÅFEGO =====\n  const estrategia_trafego = {\n    budget_sugerido: extractNumber(bigIdea, /budget.*?R\\$\\s*([\\d.,]+)/i),\n    distribuicao: {},\n    metas: {\n      cpl: extractWithUnit(bigIdea, /cpl.*?(R\\$\\s*[\\d.,\\-]+)/i),\n      cpa: extractWit