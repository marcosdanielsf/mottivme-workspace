{
  "name": "highlevel/sample-app-sql-php",
  "description": "HighLevel SDK PHP sample app with SQL session storage",
  "type": "project",
  "keywords": [
    "highlevel",
    "sql",
    "mysql",
    "sdk",
    "php",
    "oauth"
  ],
  "authors": [
    {
      "name": "HighLevel",
      "email": "marketplace@gohighlevel.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "gohighlevel/api-client": "1.0.0-beta",
    "nyholm/psr7": "^1.8",
    "nyholm/psr7-server": "^1.1",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12",
    "slim/twig-view": "^3.4",
    "twig/twig": "^3.7",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4"
  },
  "scripts": {
    "start": "php start.php"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "process-timeout": 0
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
