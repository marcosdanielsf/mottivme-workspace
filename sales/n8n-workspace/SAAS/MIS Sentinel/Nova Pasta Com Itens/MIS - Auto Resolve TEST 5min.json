        {
            "parameters": {
                "jsCode": "// Extrair issues que j√° tiveram resposta h√° mais de 5 MINUTOS (para teste)\nconst response = $input.first().json;\n\nif (!response.success || !response.issues || response.issues.length === 0) {\n  return [];\n}\n\nconst now = new Date();\nconst FIVE_MINUTES_MS = 5 * 60 * 1000; // 5 minutos para teste\n\n// Filtrar issues que:\n// 1. J√° receberam primeira resposta (first_response_at n√£o √© null)\n// 2. Resposta foi h√° mais de 5 minutos\n// 3. Ainda n√£o foram resolvidos\nconst eligibleIssues = response.issues.filter(issue => {\n  if (!issue.first_response_at || issue.resolved_at) {\n    return false;\n  }\n  \n  const responseTime = new Date(issue.first_response_at);\n  const timeSinceResponse = now - responseTime;\n  \n  return timeSinceResponse > FIVE_MINUTES_MS;\n});\n\nif (eligibleIssues.length === 0) {\n  return [];\n}\n\n// Retornar cada issue como item separado\nreturn eligibleIssues.map(issue => ({\n  json: {\n    ...issue,\n    time_since_response_hours: Math.floor((now - new Date(issue.first_response_at)) / (60 * 60 * 1000)),\n    time_since_response_minutes: Math.floor((now - new Date(issue.first_response_at)) / (60 * 1000))\n  }\n}));"
            },
